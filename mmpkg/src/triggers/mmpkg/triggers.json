                    {
                        "pattern": "^\\[(\\d+)\\] people in (.*) heard you relay (.*)$",
                        "type": "regex"
                    },
                    {
                        "pattern": "^(\\w+)\\@\\#(.*)$",
                        "type": "regex"
                    }
                ],
                "script": "selectCurrentLine() mmpkg.Captures:append(\"Relays\") deselect() resetFormat()"
            },
            {
                "name": "Form",
                "patterns": [
                    {
                        "pattern": "(.*) tells the formation (.*)",
                        "type": "regex"
                    },
                    {
                        "pattern": "^You tell the formation (.*)",
                        "type": "regex"
                    }
                ],
                "script": "selectCurrentLine() mmpkg.Captures:append(\"Form\") deselect() resetFormat()"
            },
            {
                "name": "Tells",
                "patterns": [
                    {
                        "pattern": "^You tell (.*)$",
                        "type": "regex"
                    },
                    {
                        "pattern": "^(.*) (tells|impresses upon) you '(.*)'$",
                        "type": "regex"
                    }
                ],
                "script": "if not string.starts(matches[2],\"the formation\") then selectCurrentLine() mmpkg.Captures:append(\"Tells\") deselect() resetFormat() end"
            },
            {
                "name": "Paging",
                "patterns": [
                    {
                        "pattern": "^\\a(\\w+) is paging you...$",
                        "type": "regex"
                    }
                ],
                "script": "mmpkg.pagedby = matches[2]",
                "children":[
                    {
                        "name": "Reason",
                        "patterns":[
                            {
                                "pattern": "^Reason: (.*)$",
