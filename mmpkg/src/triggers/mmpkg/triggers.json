    {
        "name": "Captures",
        "isFolder": "yes",
        "children": [
            {
                "name": "Clan",
                "patterns": [
                    {
                        "pattern": "^\\[CLAN\\] (.*)$",
                        "type": "regex"
                    },
                    {
                        "pattern": "^\\[(.*)\\] clan members heard you say, (.*)$",
                        "type": "regex"
                    }
                ],
                "script": "selectCurrentLine() mmpkg.Captures:append(\"Clan\") deselect() resetFormat()"
            },
            {
                "name": "Novice Clan",
                "highlight": "yes",
                "highlightFG": "#d5d56a",
                "highlightBG": "#000000",
                "patterns": [
                    {
                        "pattern": "^(\\[CLAN Novice Adventurers\\] .*)$",
                        "type": "regex"
                    },
                    {
                        "pattern": "^(\\[.*\\] Novice clan members heard you say, .*)$",
                        "type": "regex"
                    }
                ],
                "script": "selectCurrentLine() mmpkg.Captures:append(\"Novice\") deselect() resetFormat()"
            },
            {
                "name": "Alliance Chat",
                "patterns": [
                    {
                        "pattern": "^(\\[ALLIED.*\\] .*)$",
                        "type": "regex"
                    },
                    {
                        "pattern": "^(\\[\\d+\\] alliance members heard you say, .*)$",
                        "type": "regex"
                    }
                ],
                "script": "selectCurrentLine() mmpkg.Captures:append(\"Alliance\") deselect() resetFormat()"
            },
            {
                "name": "Talk",
